    "initial": "onLoaded",
    "states": {
        "onLoaded": {
            "on": {
                "TABITEMS_INIT": "tabItemsInit"
            }
        },
        "tabItemsInit": {
            "onEntry": [
                "onTabItemsInit"
            ],
            "on": {
